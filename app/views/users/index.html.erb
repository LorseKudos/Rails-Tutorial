<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= form_for(:search, url: users_path, remote: true) do |f| %>
  <%= f.text_field :query, value: "#{@str}", id: 'search_form' %>
  <%= f.submit "Search", class: "btn btn-primary" %>
<% end %>

<div id = "users_list">
  <%= render 'users/users_list' %>
</div>
